void setup()
{
	int i;
	for (i = 0; i < cl; i++)
		pinMode(pins[i], OUTPUT);
}

void loop()
{
	int a,b,c;
	
	for (a = 0; a < fm; a++)
	{
		for (b = 0; b < fl; b++)
		{
			for (c = 0; c < cl; c++)
			{
				if (da[a*fl*cl + b*cl + c] == 0) {digitalWrite(pins[c], LOW);}
				else {digitalWrite(pins[c], HIGH);}
			}
		}
		for (c = 0; c < cl; c++)
                delayMicroseconds(dt[a]);
		{digitalWrite(pins[c], LOW);}
		
	}
}
